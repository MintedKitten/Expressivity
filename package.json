{
  "name": "expressivity",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node build/bin/www.mjs",
    "build:tscbuild": "npx tsc bin/www.ts --outDir build --esModuleInterop --target esnext --moduleResolution node",
    "build:esbuild": "esbuild esbuild.config.ts --bundle --format=esm --platform=node --outfile=esbuild.config.mjs --external:./node_modules/* --target=ESNEXT --minify --allow-overwrite && node esbuild.config.mjs",
    "dev": "nodemon build/bin/www.mjs"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "acorn": "^8.8.2",
    "acorn-jsx": "^5.3.2",
    "esbuild": "^0.16.17",
    "esbuild-node-externals": "^1.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "turbo": "^1.7.2"
  }
}
